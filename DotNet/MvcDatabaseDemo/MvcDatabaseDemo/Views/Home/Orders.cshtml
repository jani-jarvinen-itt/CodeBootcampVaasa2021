@{
    ViewData["Title"] = "Northwind Orders";
}
<h1>@ViewData["Title"]</h1>

<table>
    <thead>
        <tr>
            <th>Order Id</th>
            <th>Order Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (MvcDatabaseDemo.Models.Order order in ViewBag.AllOrders)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.OrderDate</td>
            </tr>
        }
    </tbody>
</table>

